    </item>
    <item>
      Cennik <tag>out="price-list";</tag>
    </item>
    <item>
      Jakie są ceny <tag>out="price-list";</tag>
    </item>
    <item>
      Jakie są bronie <tag>out="weapon-list";</tag>
    </item>
    <item>
      Tak <tag>out="yes";</tag>
    </item>
	    <item>
      <ruleref uri="#bronie"/>
    </item>
    </one-of>
  </rule>
  
  <rule id="amunicjazbronia">
    <item repeat ="0-1">
      <ruleref uri="#liczby"/>
      <tag>out = out + ":" + rules.latest();</tag>
    </item>
    <item repeat ="0-1">naboje z</item>
    <item repeat ="0-1">naboi z</item>
    <item repeat ="0-1">naboi</item>
      <item repeat ="0-1">
        <ruleref uri="#bronie" />
        <tag>out = out + ":" + rules.latest();</tag>
      </item>
  </rule>
  
  <rule id="bronzamunicja">
    <item repeat ="0-1">
      <ruleref uri="#bronie" />
      <tag>out = out + ":" + rules.latest();</tag>
    </item>
      <item repeat ="0-1">
        <ruleref uri="#liczby" />
        <tag>out = out + ":" + rules.latest();</tag>
      </item>
    <item>naboi</item>
  </rule>
  
