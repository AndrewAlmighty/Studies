         tag-format="semantics/1.0" root="rootRule">

  <rule id="rootRule">
    <item repeat ="0-1">
      <ruleref uri="#rejestracja" />
      <tag> out.rejestracja=rules.latest(); </tag>
    </item>
    <item repeat ="0-1">
      <ruleref uri="#liczby" />
      <tag> out.liczby=rules.latest(); </tag>
    </item>
    <item repeat ="0-1">
      <ruleref uri="#termin"/>
      <tag> out.termin=rules.latest(); </tag>
    </item>
    <item repeat ="0-1">
    <ruleref uri="#bronzamunicja"/>
      <tag> out.strzelanie=rules.latest(); </tag>
    </item>
    <item repeat ="0-1">
    <ruleref uri="#amunicjazbronia"/>
      <tag> out.strzelanie=rules.latest(); </tag>
    </item>
  </rule>

  <rule id="rejestracja">
    <one-of>
    <item>
      zakończ <tag>out="finish";</tag>
    </item>
    <item>
      powrót <tag>out="return";</tag>
    </item>
    <item>
      powtórz <tag>out="again";</tag>
    </item>
    <item>
      opcja pierwsza <tag>out="register";</tag>
    </item>
    <item>
